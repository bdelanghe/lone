{
  "name": "semantic-page-elements",
  "dockerComposeFile": "../compose.yml",
  "service": "dev",
  "workspaceFolder": "/workspace",
  "shutdownAction": "stopCompose",
  "forwardPorts": [3000],
  "postStartCommand": "bash -lc '/workspace/scripts/devcontainer-post-start.sh'",
  "features": {},
  "customizations": {
    "vscode": {
      "extensions": [
        "denoland.vscode-deno"
      ],
      "settings": {
        "deno.enable": true,
        "deno.lint": true,
        "deno.unstable": false
      }
    }
  },
  "remoteUser": "vscode"
}
